#! /usr/bin/bash
-e # stop if it fails at any point

#echo "==== setup"
CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="setup"
printf "${CYAN}==== ${LABEL}${NO_COLOR}\n"

bin_path="$(realpath .)/bin"

source "$bin_path/db/drop"
