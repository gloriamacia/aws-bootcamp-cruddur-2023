#! /usr/bin/bash

docker build \
--build-arg REACT_APP_BACKEND_URL="api.cruddur.ch" \
--build-arg REACT_APP_AWS_PROJECT_REGION="$AWS_DEFAULT_REGION" \
--build-arg REACT_APP_AWS_COGNITO_REGION="$AWS_DEFAULT_REGION" \
--build-arg REACT_APP_AWS_USER_POOLS_ID="eu-west-1_uUf6gbNlT" \
 --build-arg REACT_APP_CLIENT_ID="tgcsakira08cvgthsjd61pob4" \
 -t frontend-react-js \
 -f Dockerfile.prod .